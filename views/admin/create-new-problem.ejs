    <script>
        $('#current-breadcrumb').append('<li class="breadcrumb-item">Admin</li><li class="breadcrumb-item">List all user</li>');//sub header\

        //get page from url
        var url_string = window.location.href; //window.location.href
        var url = new URL(url_string);
        var page = parseInt(url.searchParams.get("page"));


        //set number of page in Pre and Next button
        var firstPage = 1;
  
        if (page == firstPage) {
            //  $('#pre-page').prop( "disabled", true );
            document.getElementById("pre-page").onclick = function () { return false; };
        }
        else {
            var prePage = page - 1;
            $("#pre-page").attr("href", '/admin/list-all-user?page=' + prePage);
        }
        var nextPage = page + 1;
        $("#next-page").attr("href", '/admin/list-all-user?page=' + nextPage);
    </script>

